package com.example.demo.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class User {
	
	@Id 
	private int id;
	
	public User() {
		// TODO Auto-generated constructor stub
	}
	
	public User(int id) {
		this.id = id;
	}
	
	public void setId(int id) {
		this.id = id;
	}
	
	public int getId(int id) {
		return this.id;
	}
	
}
